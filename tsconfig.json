{
  "compilerOptions": {
    "target": "ES2020",
    "resolveJsonModule": true,
    "module": "CommonJS",
    "rootDir": "./src",
    "outDir": "./dist",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "baseUrl": "./src",
    "paths": {
      "@/*": ["*"]
    }
  },
  "include": ["./src/**/*.ts", "./prisma/seed.ts"]
}
